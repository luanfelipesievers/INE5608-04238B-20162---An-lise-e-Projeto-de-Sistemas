<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet JMetroDarkTheme.css?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" maxHeight="768.0" maxWidth="1024.0" minHeight="768.0"
            minWidth="1024.0" prefHeight="768.0" prefWidth="1024.0" styleClass="background"
            stylesheets="@../styles/JMetroLightTheme.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
            xmlns="http://javafx.com/javafx/8"
            fx:controller="br.com.moviecomander.funcionario.controller.RetirarTicketFXMLController">
    <children>
        <Label layoutX="92.0" layoutY="56.0" styleClass="header" text="Insira os dados referentes a reserva do cliente">
            <font>
                <Font name="Segoe UI" size="30.0"/>
            </font>
        </Label>
        <Text fx:id="erro" fill="RED" layoutY="156.0" stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.0"
              textAlignment="CENTER" wrappingWidth="1024.0"/>

        <Label layoutX="337.0" layoutY="345.0" styleClass="item-title" text="CPF" textFill="WHITE">
            <font>
                <Font name="Segoe UI" size="12.0" fx:id="x11"/>
            </font>
        </Label>
        <TextField fx:id="inputCPF" layoutX="382.0" layoutY="335.0" prefHeight="35.0" prefWidth="314.0"
                   promptText="Digite o CPF"/>
        <Label layoutX="328.0" layoutY="409.0" styleClass="item-title" text="Código" textFill="WHITE"
               AnchorPane.rightAnchor="657.0">
            <font>
                <Font name="Segoe UI" size="12.0" fx:id="x111"/>
            </font>
        </Label>
        <TextField fx:id="inputCodigo" layoutX="382.0" layoutY="399.0" prefHeight="35.0" prefWidth="314.0"
                   promptText="Digite o código"/>
        <Button fx:id="btnBuscar" layoutX="367.0" layoutY="667.0" mnemonicParsing="false" onAction="#handleButtonBuscar"
                text="Buscar"/>
        <Button layoutX="577.0" layoutY="667.0" mnemonicParsing="false" onAction="#handleButtonCancelar"
                text="Cancelar"/>


    </children>
</AnchorPane>
